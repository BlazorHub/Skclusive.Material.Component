@namespace Skclusive.Material.Menu
@inherits MenuComponent

 <Popover
    Class="@_Class"
    Style="@_Style"
    RootRef="@RootRef"
    Open="@Open"
    OnClose="@HandleClose"
    AnchorRef="@AnchorRef"
    ContentAnchorRef="@ContentAnchorRef"
    AnchorVerticalOrigin="@VerticalOrigin.Top"
    AnchorHorizontalOrigin="@HorizontalOrigin.Left"
    TransformVerticalOrigin="@VerticalOrigin.Top"
    TransformHorizontalOrigin="@HorizontalOrigin.Left"
    TransitionDuration="@TransitionDuration"
    AppearTimeout="@AppearTimeout"
    EnterTimeout="@EnterTimeout"
    ExitTimeout="@ExitTimeout"
    MountOnEnter="@MountOnEnter"
    UnmountOnExit="@UnmountOnExit"
    OnEnter="@HandleEnter"
    OnExited="@HandleExited"
    OnEntering="@HandleEntering"
    OnEntered="@HandleEntered"
    OnExit="@HandleExit"
    OnExiting="@HandleExiting"
    PaperClass="@(ToClass("Paper"))">
    <MenuList
        Class="@_ListClass"
        Style="@_ListStyle"
        RootRef="@ListRef"
        Open="@Open"
        Variant="@Variant"
        AutoFocus="@ListAutoFocus"
        AutoFocusItem="@AutoFocusItem"
        DisableListWrap="@DisableListWrap"
        ContentAnchorRef="@ContentAnchorRef"
        OnKeyDown="@HandleKeyDownAsync">
        @ChildContent
    </MenuList>
</Popover>